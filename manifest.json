{
  "name": "Where Was I",
  "description": "A Chrome extension that remembers your search sessions and captures context.",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "assets/logo.png"
  },
  "background": {
    "service_worker": "background/worker.js",
    "type": "module"
  },
  "permissions": ["tabs", "storage", "windows"],
  "host_permissions": ["<all_urls>"],
  "chrome_url_overrides": {
    "newtab": "dashboard/index.html"
  }
}
